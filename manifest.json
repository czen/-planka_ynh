{
    "name": "Planka",
    "id": "planka",
    "packaging_format": 1,
    "description": {
        "en": "Free open source kanban board for workgroups",
        "fr": "Free open source kanban board for workgroups"
    },
    "version": "1.1.5~ynh1",
    "url": "https://planka.app/",
    "upstream": {
        "license": "MIT",
        "website": "https://planka.app/",
        "demo": "https://plankanban.github.io/planka/#/",
        "admindoc": "https://yunohost.org/packaging_apps",
        "code": "https://github.com/plankanban/planka"
    },
    "license": "MIT",
    "maintainer": {
        "name": "",
        "email": ""
    },
    "requirements": {
        "yunohost": ">= 4.3.0"
    },
    "multi_instance": true,
    "services": [
        "nginx",
        "mysql"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/planka",
                "default": "/planka"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
